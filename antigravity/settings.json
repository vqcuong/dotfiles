{
  "accessibility.signals.terminalBell": {
    "sound": "on"
  },
  "better-comments.highlightPlainText": true,
  "better-comments.multilineComments": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "explorer.confirmDelete": false,
  "explorer.autoReveal": true,
  "explorer.excludeGitIgnore": true,
  "explorer.compactFolders": true,
  "explorer.confirmDragAndDrop": false,
  "errorLens.statusBarColorsEnabled": true,
  "errorLens.enabledDiagnosticLevels": ["error", "warning", "info", "hint"],
  "files.associations": {
    ".env*": "dotenv",
    "*.yml": "dockercompose"
  },
  "update.showReleaseNotes": false,
  "update.mode": "manual",
  "editor.acceptSuggestionOnCommitCharacter": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.accessibilitySupport": "off",
  "editor.autoClosingQuotes": "always",
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.bracketPairColorization.enabled": true,
  "editor.find.addExtraSpaceOnTop": true,
  "editor.find.globalFindClipboard": true,
  "editor.fontFamily": "fuzzy bubbles",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.formatOnSave": false,
  "editor.guides.bracketPairs": true,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.guides.highlightActiveIndentation": true,
  "editor.inlayHints.enabled": "on",
  "editor.inlineSuggest.enabled": true,
  "editor.inlayHints.fontFamily": "Monofur Nerd Font Propo",
  "editor.inlayHints.fontSize": 12,
  "editor.matchBrackets": "always",
  "editor.minimap.enabled": false,
  "editor.minimap.size": "fit",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.occurrencesHighlight": "multiFile",
  "editor.overviewRulerBorder": false,
  "editor.parameterHints.enabled": true,
  "editor.rename.enablePreview": true,
  "editor.roundedSelection": true,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.semanticHighlighting.enabled": true,
  "editor.smartSelect.selectSubwords": true,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggest.preview": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "off",
  "editor.tabSize": 4,
  "editor.detectIndentation": false,
  "editor.wordWrap": "off",
  "editor.tokenColorCustomizations": {
    "[Transparent Color Theme]": {
      "textMateRules": [
        {
          "scope": "source",
          "settings": {
            "foreground": "#faffa3"
          }
        },
        {
          "scope": ["comment"],
          "settings": {
            "foreground": "#4b7cb0",
            "fontStyle": "italic"
          }
        },
        {
          "scope": ["comment.block", "comment.block.documentation"],
          "settings": {
            "foreground": "#774bb0",
            "fontStyle": "italic"
          }
        },
        {
          "scope": [
            "keyword",
            "keyword.control",
            "keyword.operator",
            "storage.type",
            "storage.modifier"
          ],
          "settings": {
            "foreground": "#ff2877",
            "fontStyle": "italic"
          }
        },
        {
          "scope": ["string.quoted.docstring"],
          "settings": {
            "foreground": "#2c6b56"
          }
        },
        {
          "scope": [
            "string",
            "punctuation.definition.string.begin",
            "punctuation.definition.string.end",
            "punctuation.separator",
            "punctuation.separator.continuation",
            "punctuation.terminator"
          ],
          "settings": {
            "foreground": "#ff7048"
          }
        },
        {
          "scope": "support.type.property-name",
          "settings": {
            "foreground": "#87e3ff",
            "fontStyle": "bold"
          }
        },
        {
          "scope": ["entity.name.class", "entity.name.type.class"],
          "settings": {
            "foreground": "#00fbff",
            "fontStyle": "bold"
          }
        },
        {
          "scope": ["entity.name.import"],
          "settings": {
            "foreground": "#00ff6e",
            "fontStyle": "italic"
          }
        },
        {
          "scope": ["entity.alias"],
          "settings": {
            "foreground": "#f187ff",
            "fontStyle": "italic"
          }
        },
        {
          "scope": [
            "entity.name.function",
            "entity.name.method",
            "meta.function",
            "meta.function-call",
            "meta.function-call.generic",
            "keyword.operator.function.infix"
          ],
          "settings": {
            "foreground": "#0084ff"
          }
        },
        {
          "scope": "variable.parameter",
          "settings": {
            "foreground": "#00ff6e",
            "fontStyle": "italic"
          }
        },
        {
          "scope": ["variable.parameter.function.language.special"],
          "settings": {
            "foreground": "#b6ff41"
          }
        },
        {
          "scope": [
            "variable.other.property",
            "variable.other.property.static"
          ],
          "settings": {
            "foreground": "#ffff00"
          }
        }
      ]
    },
